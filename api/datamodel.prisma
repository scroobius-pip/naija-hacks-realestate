
type User {
id: ID! @id
email: String! @unique
phone: String!
name: String! @unique
password: String!
properties: [Property!] @relation(link: INLINE)
type:UserType!
}

type Property {
  id: ID! @id
  title: String!
  city: String!
  state: String!
  costValue: Int!
  costType:CostType!
  owner: User!
  images: [String!]!
  description: String!
}

enum CostType {
  Rent
  Sale
}


enum UserType {
Agency
Individual
}