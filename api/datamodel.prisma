
type User {
id: ID! @id
email: String! @unique
phone: String!
name: String! @unique
password: String!
properties: [Property!] @relation(link: INLINE)
type:UserType!
cac: String @unique
tin: String @unique
contacts: [Contact!] @relation(link: INLINE) 
}

type Contact {
  id: ID! @id
  name: String!
  number: String!
  property: Property! @relation(link: INLINE)
}

type Property {
  id: ID! @id
  title: String!
  city: String!
  state: String!
  costValue: Int!
  costType:CostType!
  ownerId: ID!
  ownerName: String!
  images: [String!]!
  description: String!
}

enum CostType {
  Rent
  Sale
}


enum UserType {
Agency
Individual
}