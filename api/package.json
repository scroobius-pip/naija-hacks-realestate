    "apollo-server-core": "^2.10.0",
    "apollo-server-micro": "^2.10.1",
    "bcryptjs": "^2.4.3",
    "fecha": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "micro-dev": "^3.0.0",
    "nexus": "^0.12.0-rc.5",
    "nexus-prisma": "^0.3.8",
